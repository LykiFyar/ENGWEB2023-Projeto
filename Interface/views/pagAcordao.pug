extends layout
block content
    .w3-card-4
    header.w3-container.w3-teal
        h1 ID Acordão: #{acordao._id}
        a.w3-bar-item.w3-button(href="/edit/"+acordao._id) EDIT

    br          
    .w3-container.w3-padding
        h4 Processo: #{acordao.Processo}
        h4 Data do Acordão: #{acordao['Data do Acordão']}
        h4 Relator: #{acordao.Relator}
        h4 Descritores: #{acordao.Descritores}            
        h4 Tribunal: #{acordao.tribunal}
        h4 Url: #{acordao.url}
        each value, key in acordao
            if value && !['_id','Processo','Data do Acordão','Relator','Descritores','tribunal','url'].includes(key)
                h4 #{key}: #{value}      
    footer.w3-container.w3-teal 
        h5 &COPY Generated by João Braga, Rafael Ferreira, Marta Sá :: EngWeb2023 - #[a(href="/acordaos") Voltar]
