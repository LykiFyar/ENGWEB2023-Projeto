extends layout
block content
  .w3-card-4
    header.w3-top.w3-wide
      .w3-bar.w3-teal.w3-wide.w3-padding.w3-card
        a.w3-bar-item
          img(src='/images/favicn.ico', alt='Logo')
          | EW - Sistema de Acordãos
        // Float links to the right. Hide them on small screens
        .w3-right.w3-hide-small
          a.w3-bar-item.w3-button(href='#about') LOGIN
          a.w3-bar-item.w3-button(href='#contact') FAQ   
    .w3-container
      table.w3-table-all
        tr
          th Processo
          th Data
          th Relator
          th Descritores
        each processo in processos
          tr
            td= processo.Processo
                a(href="/retrieveList/" + processo._id)
            td= processo['Data do Acordão']
            td= processo.Relator
            td= processo.Descritores
            if admin
                td
                    button.w3-btn.w3-round.w3-indigo.w3-margin-right  
                        a(href="/retrieveList/" + processo._id) Edit
                    button.w3-btn.w3-round.w3-indigo.w3-margin-right  
                        a(href="#") Delete
              
    footer.w3-container.w3-teal
      h5 Generated by shopList::EngWeb2023 in #{d}