extends layout
block content
    .w3-card-4
        header.w3-container.w3-teal
            h2 Formulário Acordão
        
        form.w3-container(method="POST" action="/add")
            fieldset
                legend Metadata
                label Id
                input.w3-input.w3-round(type="text" name="_id")
                label Processo
                input.w3-input.w3-round(type="text" name="Processo",required)
                label Relator
                input.w3-input.w3-round(type="text" name="Relator",required)
            button.w3-btn.w3-teal.w3-mb-2(type="submit") Criar

        footer.w3-container.w3-teal
            h5 Generated by EngWeb2023 in #{d} - #[a(href="/acordaos") Return]
