extends layout 
block content 

    .w3-card-4
        header.w3-container.w3-teal
            h2 Editar Acordão
        form.w3-container(method="POST" action="/edit")
            fieldset
                legend Metadata
                label Id
                input.w3-input.w3-round(type="text" name="id" value=a._id readonly)
                label Processo
                input.w3-input.w3-round(type="text" name="processo" value=a.processo)                        
            button.w3-btn.w3-teal.w3-mb-2(type="submit") Ok
               
        footer.w3-container.w3-teal
            h5 &COPY Generated by João Braga, Rafael Ferreira, Marta Sá :: EngWeb2023 - #[a(href="/acordaos") Voltar]
                